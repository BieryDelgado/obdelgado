int main(int argc, char** argv){
	int n=5;
	for (int i=1;i<=n;i++){
		printf("Suma%d", suma(i));
		printf("factorial = %d",factorial(i));
	}
	return 0;
}


#include<iostream>

void factorial::factorial(int n){
	
}
int suma (int n){
	if(n==0)
	return 0;
	else{
		return n+suma(n-1);
	}
}#include<iostream>
